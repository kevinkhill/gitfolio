{
  "name": "gitfolio",
  "version": "0.1.0",
  "description": "Github API client for collecting gitfolio files",
  "main": "./dist/cli.js",
  "bin": "./dist/cli.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "me": "tsnd ./src/cli.ts --username kevinkhill",
    "clean": "rimraf dist/*",
    "build": "yarn clean && yarn tsc -b src",
    "bundle": "tsup ./src/cli.ts ./src/GitFolio.ts",
    "docs": "typedoc --out docs --theme minimal ./src",
    "start": "rollup -c rollup.config.ts -w",
    "site": "eleventy"
  },
  "keywords": [
    "git",
    "portfolio"
  ],
  "author": {
    "name": "Kevin Hill",
    "email": "kevinkhill@gmail.com",
    "url": "https://kevinhill.codes"
  },
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^18.3.1",
    "async": "^3.2.0",
    "clipanion": "^3.0.0-rc.10",
    "dotenv": "^8.2.0",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@octokit/types": "^6.12.0",
    "@types/async": "^3.2.5",
    "@types/js-yaml": "^4.0.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.6",
    "tsup": "^4.6.1",
    "typedoc": "^0.20.30",
    "typescript": "^4.2.2"
  }
}