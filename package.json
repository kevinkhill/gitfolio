{
  "name": "gitregator",
  "version": "1.0.1",
  "description": "Client for collecting information from Github projects.",
  "keywords": [
    "git",
    "github"
  ],
  "repository": "github:kevinkhill/gitregator",
  "license": "MIT",
  "author": {
    "name": "Kevin Hill",
    "email": "kevinkhill@gmail.com",
    "url": "https://kevinhill.codes"
  },
  "main": "./dist/gitregator.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn tsc",
    "clean": "rimraf dist/*",
    "docs": "typedoc --out docs --theme minimal ./src",
    "lint": "eslint ./src/**/*.ts",
    "test": "jest"
  },
  "release": {
    "plugins": [
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "@octokit/rest": "^18.3.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "async": "^3.2.0",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.21",
    "semantic-release": "^17.4.2"
  },
  "devDependencies": {
    "@octokit/types": "^6.12.0",
    "@types/async": "^3.2.5",
    "@types/jest": "^26.0.20",
    "@types/js-yaml": "^4.0.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.2.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "np": "^7.4.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.3",
    "ts-node-dev": "^1.1.6",
    "typedoc": "^0.20.30",
    "typescript": "^4.2.3"
  }
}
