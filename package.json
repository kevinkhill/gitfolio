{
  "name": "gitfolio",
  "version": "0.1.0",
  "description": "Github API client for collecting gitfolio files",
  "main": "dist/gitfolio.js",
  "typings": "dist/types/gitfolio.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "me": "tsnd --quiet ./src/cli.ts --username kevinkhill --json",
    "prebuild": "rimraf dist",
