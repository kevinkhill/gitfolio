    "@octokit/rest": "^18.3.1",
    "async": "^3.2.0",
    "clipanion": "^3.0.0-rc.10",
    "dotenv": "^8.2.0",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.21",
    "step": "^1.0.0"
  },
  "devDependencies": {
    "@octokit/types": "^6.12.0",
    "@types/async": "^3.2.5",
    "@types/js-yaml": "^4.0.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "hugo-cli": "^0.11.0",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.6",
